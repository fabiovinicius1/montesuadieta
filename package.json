{
  "name": "montesuadieta",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "backup:testdb": "cp prisma/teste.db prisma/teste.db.bak",
    "restore:testdb": "cp prisma/teste.db.bak prisma/teste.db",
    "start": "node dist/server.js",
    "dev": "dotenv -e .env -- ts-node-dev src/server.ts",
    "migrate:dev": "dotenv -e .env -- npx prisma migrate deploy",
    "teste": "dotenv -e .env.test.local -- npx vitest && npm run restore:testdb",
    "migrate:teste": "dotenv -e .env.test.local -- npx prisma migrate deploy && npm run backup:testdb"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "gulp": "^5.0.0",
    "gulp-nodemon": "^2.5.0",
    "gulp-tslint": "^8.1.4",
    "gulp-typescript": "^6.0.0-alpha.1",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vitest": "^2.0.5"
  }
}
